# Preparation for development environment.

# Google Cloud SDK.
GOOGLE_CLOUD_SDK=$HOME/google-cloud-sdk

# Google Go language SDK.
# Somehow the stupid system overrides these. Keep them here for our
# installation to work.
export GOROOT=/usr/local/bhojo/go
export GOPATH=/usr/local/bhojo/golib

# Google Dart SDK
DART_SDK=$HOME/dart-sdk

# Set the path environment now.
export PATH=$DART_SDK/bin:$GOPATH/bin:$GOROOT/bin:$GOOGLE_CLOUD_SDK/bin:$PATH
export PATH=$HOME/var/bin:$PATH
